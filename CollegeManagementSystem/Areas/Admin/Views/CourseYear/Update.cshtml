@using CMS.CommonHelper;
@model CMS.Models.CourseYear

@{

    ViewData["Title"] = "Central College";
    TempData["Title"] = "Course Year";

}

<div>

    <div class="row mb-4">

        <div class="col-12">

            <h2 class="m-0">Edit Course Year</h2>

        </div>

    </div>

    <div class="row">

        <div class="col-12">

            <form method="post">
                <input type="hidden" asp-for="Id" />
                <label asp-for="Year" class="mt-3">Year</label>
                <input asp-for="Year" class="form-control" />
                <span asp-validation-for="Year"></span>

                <button type="submit" class="btn custom-btn mt-2 mb-1">Save</button>

            </form>

            <a class="btn btn-secondary mt-2" asp-area="Admin" asp-controller="Semester" asp-action="Index">Back to list</a>

        </div>

    </div>


</div>

@section scripts {

    @{
        <partial name="_ValidationScriptsPartial" />

    }

}